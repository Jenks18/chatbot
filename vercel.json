{
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/admin/:path*",
      "destination": "/api/admin"
    },
    {
      "source": "/api/chat",
      "destination": "/api/chat"
    },
    {
      "source": "/api/health",
      "destination": "/api/health"
    }
  ],
  "functions": {
    "api/admin.py": {
      "maxDuration": 30
    },
    "api/chat.py": {
      "maxDuration": 60
    },
    "api/health.py": {
      "maxDuration": 10
    }
  }
}
